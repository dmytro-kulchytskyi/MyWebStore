@model MyStore.Mvc.Models.ProductViewModels.ProductsListViewModel

<datalist pageSize="@Model.PageSize" itemsCount="@Model.Items.Count" pageNumber="@Model.PageNumber" orderField="@Model.OrderField" inverseOrder="@Model.InverseOrder.ToString()" style="display: none;"></datalist>
@foreach (var product in Model.Items)
{
    @Html.Partial("ProductListItemPartial", product);
}